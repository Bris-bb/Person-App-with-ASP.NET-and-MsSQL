@model IEnumerable<PersonApp.Models.Personal>

@{
    Layout = null;
}
@Html.Partial("header", "Report")

<style>
    #sample_1 td img {
        width: 30px;
    }

    .notify {
        background-color: #d1e7fc !important;
    }

    .filter_container {
        font-size: 16px;
        border: 1px solid;
        display: table;
        padding: 9px;
        width: 100%;
    }

    #sample_1_wrapper {
        border: 1px solid;
        padding: 9px;
    }
</style>

<div class="filter_container">
    <div class="col-md-4">
        <div class="form-group row">
            <label class="control-label col-md-5" for="Email">Email</label>
            <div class="col-md-7">
                <input type="hidden" class="filter_item">
                <input type="hidden" class="filter_item">
                <input type="hidden" class="filter_item">
                <input type="checkbox" class="filter_item" checked>
            </div>
        </div>
        <div class="form-group row">
            <label class="control-label col-md-5" for="Insider_Name">Insider Name</label>
            <div class="col-md-7">
                <input type="checkbox" class="filter_item" placeholder="">
            </div>
        </div>
        <div class="form-group row">
            <label class="control-label col-md-5" for="Insider_Name_arabic"></label>
            <div class="col-md-7">
                <input type="checkbox" class="filter_item" placeholder="">
            </div>
        </div>
        <div class="form-group row">
            <label class="control-label col-md-5" for="Company_Name">Company Name</label>
            <div class="col-md-7">

                <input type="checkbox" class="filter_item" placeholder="">
            </div>
        </div>
        <div class="form-group row">
            <label class="control-label col-md-5" for="Company_Name_arabic"></label>
            <div class="col-md-7">

                <input type="checkbox" class="filter_item" placeholder="">
            </div>
        </div>

        <div class="form-group row">
            <label class="control-label col-md-5" for="Position">Position</label>
            <div class="col-md-7">

                <input type="checkbox" class="filter_item" placeholder="">
            </div>
        </div>
        <div class="form-group row">
            <label class="control-label col-md-5" for="Position_arabic"></label>
            <div class="col-md-7">
                <input type="checkbox" class="filter_item" placeholder="">
            </div>
        </div>
    </div>
    <div class="col-md-4">
        <div class="form-group row">
            <label class="control-label col-md-5" for="UAE_Resident">UAE Resident</label>
            <div class="col-md-7">
                <input type="checkbox" class="filter_item" placeholder="">
            </div>
        </div>
        <div class="form-group row">
            <label class="control-label col-md-5" for="UAE_Resident_arabic"></label>
            <div class="col-md-7">
                <input type="checkbox" class="filter_item">
            </div>
        </div>
        <div class="form-group row">
            <label class="control-label col-md-5" for="Emirates_ID">Emirates ID</label>
            <div class="col-md-7">

                <input type="checkbox" class="filter_item">
            </div>
        </div>

        <div class="form-group row">
            <label class="control-label col-md-5" for="Passport_Number">Passport Number</label>
            <div class="col-md-7">

                <input type="checkbox" class="filter_item">
            </div>
        </div>
        <div class="form-group row">
            <label class="control-label col-md-5" for="Mobile_Number">Mobile Number</label>
            <div class="col-md-7">

                <input type="checkbox" class="filter_item">
            </div>
        </div>
        <div class="form-group row">
            <label class="control-label col-md-5" for="Office_Number">Office Number</label>
            <div class="col-md-7">

                <input type="checkbox" class="filter_item">
            </div>
        </div>
        <div class="form-group row">
            <label class="control-label col-md-5" for="Email_Address_2">Email Address 2</label>
            <div class="col-md-7">

                <input type="checkbox" class="filter_item">
            </div>
        </div>
    </div>
    <div class="col-md-4">
        <div class="form-group row">
            <label class="control-label col-md-5" for="Trade_License">Trade License</label>
            <div class="col-md-7">

                <input type="checkbox" class="filter_item">
            </div>
        </div>
        <div class="form-group row">
            <label class="control-label col-md-5" for="">Security Code</label>
            <div class="col-md-7">

                <input type="checkbox" class="filter_item">
            </div>
        </div>
        <div class="form-group row">
            <label class="control-label col-md-5" for="">Other Information</label>
            <div class="col-md-7">

                <input type="checkbox" class="filter_item">
            </div>
        </div>
        <div class="form-group row">
            <label class="control-label col-md-5" for="">ID Expire Date</label>
            <div class="col-md-7">

                <input type="checkbox" class="filter_item">
            </div>
        </div>
        <div class="form-group row">
            <label class="control-label col-md-5" for="">Passport Expire Date</label>
            <div class="col-md-7">

                <input type="checkbox" class="filter_item">
            </div>
        </div>
        <div class="form-group row">
            <label class="control-label col-md-5" for="">Company TL Date</label>
            <div class="col-md-7">
                <input type="checkbox" class="filter_item">
            </div>
        </div>

    </div>
</div>


